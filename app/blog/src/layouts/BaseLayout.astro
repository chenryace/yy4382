---
import HeadTag from "@layouts/HeadTag.astro";
import type { SEOProps } from "astro-seo";
import BaseBody from "./BaseBody.astro";
interface Props {
  seoConfig: SEOProps;
  stopTrack?: boolean;
}
const { seoConfig, stopTrack } = Astro.props;
---

<HeadTag seoConfig={seoConfig} stopTrack={stopTrack}>
  <BaseBody>
    <slot />
  </BaseBody>
</HeadTag>
