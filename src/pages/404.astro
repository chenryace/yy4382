  <div class="card-base">
    <h1 class="card-heading mb-4">404: 网页走丢啦！</h1>
    <div class="flex gap-4 justify-center">
      <a class="btn-regular p-2 rounded-md" href="/1">返回首页</a>
    </div>
  </div>
</BaseLayout>
